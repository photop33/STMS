x=input("number")
y=len(x)
number=0
z="1234567890"
zz=len(z)
if x[0] == '+' or x[0] == '-':
    for c in range (0,zz):
        if x[0] == z[c]:
            print("OK")
            continue 
else:
    print ("False")
        
        
for i in range (0,y):
    if x[i] == '.':
        number=number +1 
        for c in range (0,zz):
            if z[c] == x[i]:
                print (x[i], z[c],"True")
        
if number >= 2:
    print ("flase")
else:
    print ("True")
